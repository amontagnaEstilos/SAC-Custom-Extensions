{
    "id": "ai.estilos.amontagna.demo.sankey",
    "version": "2.0.0",
    "name": "Sankey",
    "description": "Sankey Chart",
    "newInstancePrefix": "SANKEY",
    "icon": "",
    "vendor": "Estilos",
    "eula": "",
    "license": "",
    "webcomponents": [{
            "kind": "main",
            "tag": "ai-estilos-amontagna-demo-sankey",
            "url": "https://amontagnaEstilos.github.io/sacWidgetSankey/resources/sankey.js",
            "integrity": "",
            "ignoreIntegrity": true
        }
    ],
    "properties": {
        "title": {
            "type": "string",
            "description": "Chart title",
            "default": "Custom Sankey Widget"
        },
        "showTitle": {
            "type": "boolean",
            "description": "Show title of the chart",
            "default": true
        },
        "sizeLabel": {
            "type": "string",
            "description": "Name of measure for bubble size",
            "default": "Size"
        },
        "colorLabel": {
            "type": "string",
            "description": "Name of measure for bubble color",
            "default": "Color"
        },
        "selectedLinkLabel": {
            "type": "string",
            "description": "Label of selected Link",
            "default": "undefined"
        },
        "selectedLinkSource": {
            "type": "string",
            "description": "Source of selected Link",
            "default": "undefined"
        },
        "selectedLinkTarget": {
            "type": "string",
            "description": "Target of selected Link",
            "default": "undefined"
        },
        "startColor": {
            "type": "string",
            "description": "Color of the bubble with lowest color value",
            "default": "#ffcd00"
        },
        "endColor": {
            "type": "string",
            "description": "Color of the bubble with highest color value",
            "default": "#b01c02"
        },
        "data_test1_sankey": {
            "type": "string",
            "description": "Data in JSON format",
            "default": "{\"nodes\":[{\"node\":0,\"name\":\"node0\"},{\"node\":1,\"name\":\"node1\"},{\"node\":2,\"name\":\"node2\"},{\"node\":3,\"name\":\"node3\"},{\"node\":4,\"name\":\"node4\"}],\"links\":[{\"source\":0,\"target\":2,\"value\":2},{\"source\":1,\"target\":2,\"value\":2},{\"source\":1,\"target\":3,\"value\":2},{\"source\":0,\"target\":4,\"value\":2},{\"source\":2,\"target\":3,\"value\":2},{\"source\":2,\"target\":4,\"value\":2},{\"source\":3,\"target\":4,\"value\":4}]}"
        },
        "data": {
            "type": "string",
            "description": "Data in JSON format",
            "default": "{\"data\":[{\"dimensions_0\":{\"id\":\"[Product_3e315003an].[Product_Catego_3o3x5e06y2].&[PC4]\",\"label\":\"Alcohol\"},\"dimensions_1\":{\"id\":\"[Location_4nm2e04531].[State_47acc246_4m5x6u3k6s].&[SA1]\",\"label\":\"California\"},\"measures_0\":{\"raw\":30720120.73,\"formatted\":\"30.72Million\",\"unit\":\"USD\"}}],\"metadata\":{\"feeds\":{\"measures\":{\"values\":[\"measures_0\"],\"type\":\"mainStructureMember\"},\"dimensions\":{\"values\":[\"dimensions_0\",\"dimensions_1\"],\"type\":\"dimension\"}},\"dimensions\":{\"dimensions_0\":{\"id\":\"Product_3e315003an\",\"description\":\"Product\"},\"dimensions_1\":{\"id\":\"Location_4nm2e04531\",\"description\":\"Location\"}},\"mainStructureMembers\":{\"measures_0\":{\"id\":\"[Account_BestRunJ_sold].[parentId].&[Discount]\",\"label\":\"Discount\"}}}}"
        },
        "width": {
            "type": "integer",
            "default": 800
        },
        "height": {
            "type": "integer",
            "default": 800
        }
    },    
    "dataBindings": {
        "myDataSource": {
          "feeds": [
            {
              "id": "dimensions",
              "description": "Dimensions",
              "type": "dimension"
            },
            {
              "id": "measures",
              "description": "Measures",
              "type": "mainStructureMember"
            }
          ]
        }
      },
    "methods": {
        "setStartColor": {
            "description": "Sets the color of the bubble with lowest value.",
            "parameters": [{
                    "name": "newColor",
                    "type": "string",
                    "description": "Color of the bubble with lowest value"
                }
            ],
            "body": "this.startColor = newColor;"
        },
        "setEndColor": {
            "description": "Sets the color of the bubble with highest value.",
            "parameters": [{
                    "name": "newColor",
                    "type": "string",
                    "description": "Color of the bubble with highest value"
                }
            ],
            "body": "this.endColor = newColor;"
        },
        "setTitleVisible": {
            "description": "Show title of the chart.",
            "parameters": [{
                    "name": "flag",
                    "type": "boolean",
                    "description": "Show title of the chart"
                }
            ],
            "body": "this.showTitle = flag;"
        },
        "setTitle": {
            "description": "Sets the title of the chart.",
            "parameters": [{
                    "name": "newValue",
                    "type": "string",
                    "description": "The new title of chart"
                }
            ],
            "body": "this.title = newValue;"
        },
        "setData": {
            "description": "Sets the data of the chart.",
            "parameters": [{
                    "name": "newJSONData",
                    "type": "string",
                    "description": "The new data of chart"
                }
            ],
            "body": "this.data = newJSONData;"
        },
        "getSelectedLinkLabel": {
            "returnType": "string",
            "description": "Returns the label of selected Link.",
            "body": "return this.selectedLinkLabel;"
        },
        "getSelectedLinkSource": {
            "returnType": "string",
            "description": "Returns the label of selected Link.",
            "body": "return this.selectedLinkSource;"
        },
        "getSelectedLinkTarget": {
            "returnType": "string",
            "description": "Returns the label of selected Link.",
            "body": "return this.selectedLinkTarget;"
        }
    },
    "events": {
        "onSelect": {
            "description": "Called when the user selects/de-selects a bubble."
        }
    }
}
